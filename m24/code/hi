; Should be used with the HD Display setup
; The first three bits mark which row should be updated

print int('000000000000000000000000',2)
print int('001001000100111001000000',2)
print int('010001000100010001000000',2)
print int('011001111100010001000000',2)
print int('100001000100010000000000',2)
print int('101001000100111001000000',2)
print int('110000000000000000000000',2)
print int('111001111111111111000000',2)

; 44000000 44244e40 44444440 4467c440 44844400 44a44e40 44c00000 44e7ffc0
